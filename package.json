{
  "name": "paradox_launcher",
  "description": "Simple minecraft launcher",
  "version": "0.1.5",
  "author": "Ridicc",
  "private": true,
  "main": "public/Main.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "custom-electron-titlebar": "^3.2.4",
    "electron-json-storage": "^4.2.0",
    "electron-util": "^0.14.2",
    "fs": "0.0.1-security",
    "minecraft-launcher-core": "^3.14.6",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.13.1",
    "react-iframe": "^1.8.0",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/Main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "typescript": "^3.9.7"
  },
  "build": {
    "appId": "nonExistentID",
    "icon": "public/images/icon.png",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
